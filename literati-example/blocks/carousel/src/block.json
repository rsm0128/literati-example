{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "literati-example/carousel",
	"version": "1.0.0",
	"title": "Carousel Block",
	"category": "widgets",
	"icon": "smiley",
	"description": "Example carousel block",
	"example": {},
	"supports": {
		"html": false,
		"allowedBlocks": [ "literati-example/carousel-inner" ]
	},
	"attributes": {
		"autoplaySpeed": {
			"type": "number",
			"default": 3000
		},
		"slides":{
			"type": "array",
			"default": [],
			"properties": {
				"image": {
					"type": "object",
					"properties": {
						"url": {
							"type": "string",
							"default": ""
						},
						"alt": {
							"type": "string",
							"default": ""
						}
					}
				},
				"heading": {
					"type": "string"
				},
				"content": {
					"type": "string"
				},
				"link": {
					"type": "object",
					"properties": {
						"text": {
							"type": "string",
							"default": ""
						},
						"url": {
							"type": "string",
							"default": ""
						}
					}
				}
			}
		}
	},
	"textdomain": "literati-example-carousel",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"render": "file:./render.php",
	"viewScript": "file:./view.js"
}
